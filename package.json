    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^8.0.0",
    "electron": "^25.0.0",
    "electron-builder": "^24.0.0",
    "typescript": "^5.0.0",
    "vite": "^4.4.0",
    "wait-on": "^7.0.0"
