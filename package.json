{
  "name": "testing-system",
  "version": "1.0.0",
  "description": "Система управления данными тестирования",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev:vite": "vite",
    "dev:electron": "electron .",
    "build": "npm run build:vite && npm run build:electron",
